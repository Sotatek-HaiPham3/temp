{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.3",
    "aporat/store-receipt-validator": "^3.4",
    "aws/aws-sdk-php": "^3.137",
    "brozot/laravel-fcm": "^1.3",
    "darkaonline/l5-swagger": "6.*",
    "doctrine/dbal": "^2.9",
    "elasticsearch/elasticsearch": "~7.0",
    "fideloper/proxy": "^4.0",
    "firebase/php-jwt": "^5.2",
    "fruitcake/laravel-cors": "^2.0",
    "gnello/php-mattermost-driver": "^2.12",
    "google/apiclient": "^2.0",
    "guzzlehttp/guzzle": "^6.5",
    "inacho/php-credit-card-validator": "^1.0",
    "intervention/image": "^2.7",
    "klaviyo/php-sdk": "^2.1",
    "laravel-notification-channels/twilio": "^3.0",
    "laravel/framework": "6.0",
    "laravel/passport": "^8.4",
    "laravel/socialite": "^4.3",
    "laravel/tinker": "^2.0",
    "lcobucci/jwt": "^3.3",
    "league/flysystem-aws-s3-v3": "^1.0",
    "martinlindhe/laravel-vue-i18n-generator": "^0.1.45",
    "mbpcoder/laravel-api-versioning": "^1.1",
    "paypal/rest-api-sdk-php": "*",
    "php-ffmpeg/php-ffmpeg": "^0.16.0",
    "phpmd/phpmd": "^2.8",
    "piwik/device-detector": "^3.11",
    "predis/predis": "^1.1",
    "propaganistas/laravel-phone": "^4.2",
    "squizlabs/php_codesniffer": "^3.5",
    "stevebauman/location": "^5.0",
    "stripe/stripe-php": "^6.31",
    "symfony/translation": "^4.3.8",
    "symfony/translation-contracts": "1.1.6",
    "twilio/sdk": "^6.21",
    "zircote/swagger-php": "2.*",
    "ext-json": "*"
  },
  "require-dev": {
    "facade/ignition": "^1.4",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^3.0",
    "phpunit/phpunit": "^8.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "files": [
      "app/helpers.php"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "database/seeds",
      "database/factories"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  }
}
